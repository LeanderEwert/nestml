neuron cm_model_three_invalid:

    state:
        # compartmental voltage variable,
        # rhs value is irrelevant but the state must exist so that the nestml parser doesn't complain
        v_comp real = 0.0
    
        m_Na real = 0.0
        h_Na real = 0.0

    #sodium
    function m_inf_Na(v_comp real) boolean:
        return true
    
    function tau_m_Na(v_comp real) integer:
        return 1111111111
    
    function h_inf_Na(v_comp real) string:
        return "hello"
    
    function tau_h_Na(v_comp real) void: 
        v_comp+=1.0
        return
    
    function some_random_function_to_ignore(v_comp real, something_else real) string: 
        return "ignore"

    equations:
        inline Na real = m_Na**3 * h_Na**1

    parameters:
        e_Na real = 50.0
        gbar_Na real = 0.0
