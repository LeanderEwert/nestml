  state:
    # indicate that we have a compartmental model
    # by declaring an unused variable with the name "v_comp"
    v_comp real = 0.0
    
    m_Na real = 0.0
    
  end

  equations: 
    inline cm_p_open_Na real = m_Na**3
    
  end

  parameters:
    e_Na real = 50.0
    gbar_Na real = 0.0
    
  end


